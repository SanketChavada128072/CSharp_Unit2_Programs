using System;

class Program
{
    static void Main()
    {
        Console.Write("Enter your marks: ");
        double marks = Convert.ToDouble(Console.ReadLine());

        if (marks > 80)
        {
            Console.WriteLine("Grade: A");
        }
        else if (marks > 60)
        {
            Console.WriteLine("Grade: B");
        }
        else if (marks > 50)
        {
            Console.WriteLine("Grade: B");
        }
        else if (marks > 40)
        {
            Console.WriteLine("Grade: D");
        }
        else
        {
            Console.WriteLine("Grade: Fail");
        }

        Console.ReadKey();
    }
}
