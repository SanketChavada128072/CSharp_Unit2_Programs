using System;

class Program
{
    static void Main()
    {
        Console.Write("Enter a number: ");
        int num = int.Parse(Console.ReadLine());

        int rev = 0;
        int temp = num;

        while (temp > 0)
        {
            rev = rev * 10 + temp % 10;
            temp /= 10;
        }

        Console.WriteLine(num == rev ? "Palindrome" : "Not Palindrome");
    }
}
